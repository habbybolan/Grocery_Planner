<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".listing.grocerylist.GroceryListActivity"
            android:orientation="vertical">

            <fragment
                android:id="@+id/fragment_recipe_list"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:name="com.habbybolan.groceryplanner.listing.grocerylist.grocerylist.GroceryListFragment"
                tools:layout="@layout/fragment_grocery_list"
                android:tag="@string/GROCERY_LIST_TAG"/>

        </LinearLayout>

    </RelativeLayout>

</layout>